{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": 0,
    "indent": 2,
    "max-len": ["error", 400],
    "react/prop-types": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["to"]
      }
    ],
    "no-unused-vars": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
